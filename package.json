{
  "name": "@kinolanka/check-my-deps",
  "version": "0.1.0",
  "description": "Easily manage and monitor project dependencies.",
  "license": "MIT",
  "keywords": [
    "npm",
    "dependencies",
    "package-management",
    "version-check",
    "outdated",
    "deprecated",
    "semver",
    "dependency-analyzer",
    "excel-export",
    "json-export",
    "cli-tool",
    "typescript",
    "node.js",
    "package.json",
    "dependency-update",
    "npm-registry",
    "check-my-deps"
  ],
  "homepage": "https://checkmydeps.com",
  "bugs": {
    "url": "https://github.com/kinolanka/check-my-deps/issues"
  },
  "author": "Volodymyr Mashkin <kinolanka@gmail.com> (https://kinolanka.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kinolanka/check-my-deps.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "start": "npm run clean && npm run build && node dist/index.js",
    "test": "jest",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "lint": "eslint --ext .ts ./src",
    "lint:fix": "eslint --ext .ts ./src --fix",
    "prepare": "husky",
    "ci": "npm run clean && npm run format:check && npm run lint && npm run typecheck && npm run test && npm run build",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "exceljs": "^4.4.0",
    "fs-extra": "^11.3.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@kinolanka/prettier-config": "^1.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^3.8.3",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.6",
    "tsup": "^8.4.0",
    "type-fest": "^4.37.0",
    "typescript": "^5.8.2"
  }
}
